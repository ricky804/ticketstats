div class="container-fluid"
  div class="row-fluid"
    div class="span2"
      <!--Sidebar content-->
      = render :partial => "particial/sidemenu3"
    div class="span10"
      <!--Here goes the body content -->
      h1 = @rtqueue.Name
      div = high_chart("my_id4", @chart)
      div = high_chart("id", @chart_ticket_count_by_email)
      table class="table"
        tr
          th Ticket
          th Subject
          th LastUpdated
          th Status
        - @open_tickets.each do |ticket|
          tr
            td = ticket.id
            td = ticket.Subject
            td = ticket.LastUpdated
            td = ticket.Status


